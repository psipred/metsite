<table width="100%">
<tr>  
<th><span style ="font-size: x-large;">Entry</span></th>
<% testsets = TestSet.find(:all) %>
<% for testset in testsets %>
    <th>
      <span style="font-size: x-large;"><%= testset.name %></span>
    </th>
  <% end %>
</tr>
<% for test_entry in TestEntry.find(:all, :order => "name ASC") %>
  <tr style="background: <%= cycle("#ccc","") %>;">
      <td><span style = "font-size: large;"><%= link_to test_entry.name, 
            { :action => 'show_entry', :id => test_entry.id } %></span></td>
      <% for testset in testsets %>
        <% checked = testset.test_entries.include?(test_entry) %>
        <td>
            <%= check_box_tag "#{test_entry.name}_#{testset.name}", checked, checked %>
            </td>
      <% end%>
    </tr>
  <% end%>
</table>