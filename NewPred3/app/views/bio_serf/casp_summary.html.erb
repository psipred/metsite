<head>
  <title>Casp 8 Summary Page</title>
  <script src="../../../javascripts/Jmol.js" type="text/javascript"></script>
</head>

<body>
  <% if (!@jobs)%>
      No CASP 8 Jobs completed yet<br>
    <% end %>
    <% @jobs.each do |job| %>
        <%= render :partial => 'job_thumbnail',
             :locals => {:job => job, :bgcolor => "#ccc"} %>
      <% end %>
      <%= will_paginate @jobs%>
  </body>