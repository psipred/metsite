<%#
# To change this template, choose Tools | Templates
# and open the template in the editor.
%>

<% data_segments = log_result.data.split(/Results:\n/) %>
<tr><td>
<table>
<% data_segments.each_with_index do |segment, index|
       if index == 1
           lines = segment.split(/\n/)
           lines.each do |line| %>
          <tr>
          <% data = line.split(/:\s+/)
             data.each_with_index do |datum, index2|
               if index2 == 0 %>
            <td style="width: 20%"><%= datum %></td>
            <% end
               if index2 == 1 %>
            <td style="width: 20%"><b><%= datum %></b></td>
            <% end
             end %>
          </tr>
        <% end
       end
   end %>
</table>
</td></tr>
