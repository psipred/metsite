<%#
# To change this template, choose Tools | Templates
# and open the template in the editor.
%>

<div style="background: <%= cycle(bgcolor,'#fff') %>; padding: 4px;">
  <strong>Type:</strong>
  <%= model.model_type.name %><br>
  <strong>Filename:</strong>
  <a href="<%= url_for :controller => "data_manager",
       :action => "getmodelattached",
       :id => model.id %>">
 <%= "#{model.test_entry.name}_#{model.model_type.name}_#{model.id}.pdb" %></a><br>
  <strong>Time:</strong>
  <%= model.created_at%><br>
  <form>
    <script type="text/javascript">
      jmolInitialize("/", "JmolApplet.jar"); 
      jmolApplet(480, "load <%= url_for( :controller => "data_manager",
                                    :action => "getmodelattached",
                                    :id => model.id) %>; cpk off; wireframe off; cartoon; color cartoon structure; ");
    </script>
  </form>
</div>
