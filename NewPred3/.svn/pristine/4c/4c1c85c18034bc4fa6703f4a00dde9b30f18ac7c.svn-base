<table>

<% #Display the metsite prediction in jmol
   @job_results.each do |job_state|
     if (job_state.status_class == 23 )%>
  <tr class="form_subtitle"><td><h4>Metsite Prediction</h4></td></tr>

  <tr><td>
    <div style="padding: 4px;">
      <form>
        <script type="text/javascript">
          jmolInitialize("/", "JmolApplet.jar");
          jmolApplet(480, "load <%= url_for( :controller => "bio_serf",
                                    :action => "getresultattached",
                                    :id => job_state.id) %>; cpk off; wireframe off; cartoon; color cartoon relativeTemperature; ");
        </script>
      </form>
    </div><p>
    <table class="colorTable" border="0" cellpadding="4" cellspacing="0" width="100%">
    <tr><td bgcolor="#ff0000" style="border-style:solid; border-width:1px; border-color:#000000">&nbsp;&nbsp;</td><td>&nbsp;Metal Binding Contact</td></tr>
    <tr><td bgcolor="#ffffff" style="border-style:solid; border-width:1px; border-color:#000000">&nbsp;&nbsp;</td><td>&nbsp;Chain not predicted</td></tr>
    <tr><td bgcolor="#0000ff" style="border-style:solid; border-width:1px; border-color:#000000">&nbsp;&nbsp;</td><td>&nbsp;Predicted chain</td></tr>

    </table>
</p></td></tr>
  <% end
   end %>

<% #print out the table of metsite scores
   @job_results.each do |job_state|
      if (job_state.status_class == 22 )%>
     <tr class="form_subtitle"><td><h4>Metsite Prediction scores</h4></td></tr>
     <tr><td>
    <table id="metsiteTable" class="tablesorter">
      <thead>
        <tr>
          <th style="width: 60px">Residue</th>
          <th style="width: 150px">Raw Neural Network Score</th>
          <th style="width: 100px">Residue</th>
        </tr>
      </thead>
      <tbody>
    <% for line in job_state.data.split(/\n/)
       line = line.gsub(/^\s+/,"") %>
    <tr>
      <% entries = line.split(/\s+/)
         entries.each do |entry| %>
        <td style="text-align: right"><%= entry %></td>
      <% end %>
    </tr>
    <% end %>
    </tbody>
    </table>
    </td></tr>
  <% end
   end %>

</table>
