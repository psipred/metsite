<%= form_tag("/paramsubmit", :method => "get") do %>
  <p>Job Root Name:
  <%= text_field_tag("name","", :class => "form_input")%> </p>

  <p>Job Type:
  <%= select_tag "job_type", "<option>BioSerf</option><option>mGenThreader</option><option>TestPred</option>NewPred<option></option>"%></p>
  <p>Sequence:
  <%= text_area_tag "sequence", "", :size => "70x5"%></p>
  <p>Configuration Parameter 1:
  <%= text_field_tag("param_name_1","", :class => "form_input")%> </p>
  <p>Param Entries (comma delim):
  <%= text_field_tag("param_entries_1","", :class => "form_input")%> </p>
  <p>Configuration Parameter 2:
  <%= text_field_tag("param_name_2","", :class => "form_input")%> </p>
  <p>Param Entries (comma delim:
  <%= text_field_tag("param_entries_2","", :class => "form_input")%> </p>
  <p>Configuration Parameter 3:
  <%= text_field_tag("param_name_3","", :class => "form_input")%> </p>
  <p>Param Entries (comma delim):
  <%= text_field_tag("param_entries_3","", :class => "form_input")%> </p>
  <p>Configuration Parameter 4:
  <%= text_field_tag("param_name_4","", :class => "form_input")%> </p>
  <p>Param Entries (comma delim):
  <%= text_field_tag("param_entries_4","", :class => "form_input")%> </p>
  <p>Configuration:<br>
  <%= select "newjob", "configuration_id", JobConfiguration.find(:all).collect { |p| [p.name, p.id ] } %></p><br>

  <%= submit_tag("Param Search", :class => "form_input")%>
<% end %>
