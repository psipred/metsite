<script src="../../javascripts/Jmol.js" type="text/javascript"></script>
<h1>Results for <%= @job.name %></h1>
<% if (!@job_results)%>
      No Job Status Updates yet...<br>
 <% end %>

</br>
 <% @job_results.each do |job_state| %>
        <% if (job_state.status_class == 6) %>
                    <% if job_state.content_name =~ /^No model built/ %>
                    <h2>Could Not Build Model</h2>
                    <h2><%= job_state.content_name %></h2>
                     <%  else %>
                    <%= render :partial => 'layouts/pdb_result', :locals => {:pdb_result => job_state, :bgcolor => "#ccc"} %>
                    <h3><a href="<%= url_for( :controller => "bio_serf", :action => "getresultattached", :id => job_state.id) %>">Download PDB</a></h3>
                    
                <% end %>


      <% end %>
 <% end %>